{
  "name": "Address",
  "plural": "Addresses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "street": {
      "type": "string",
      "required": true
    },
    "street2": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "zipcode": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "default": "USA"
    }
  },
  "validations": [],
  "relations": {
    "listings": {
      "type": "hasMany",
      "model": "Listing",
      "foreignKey": "addressId",
      "options": {
        "nestRemoting": true
      }
    },
    "owners": {
      "type": "hasMany",
      "model": "Ownership",
      "foreignKey": "addressId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
